            <!-- Data Resolution ======================================================= -->
            







    <div class="fieldSection">
        <div class="field">Data Resolution</div>
        <div>
        
        
            
            

            
                <span class="textrecord"><strong>Latitude Resolution:</strong>
                60 km width along track at nadir
                </span><br/>
            

            
                <span class="textrecord"><strong>Longitude Resolution:</strong>
                100 km width across track at nadir
                </span><br/>
            

            
                <span class="textrecord"><strong>Horizontal Resolution Range:</strong>
                250 km - <  500 km  or approximately  2.5 degrees - < 5.0 degrees
                </span><br/>
            

            

            

            
                <span class="textrecord"><strong>Temporal Resolution:</strong>
                Twice daily; ascending and descending nodes; 36-day yaw cycle
                </span><br/>
            

            

	    

                

                
		

            
        </div>
