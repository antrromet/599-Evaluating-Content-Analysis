            function doGoogleMapOpen()
            {
                gcmd.gwt.widgetwrappers.pageViewerApp.showWindow("map1");
                initGoogleMap();
            }
            
            function doGoogleMapLoad(list)
            {
                gcmd.gwt.widgetwrappers.pageViewerApp.showWindow("map1");
                loadGoogleMap(list);
            }
            
            function doneLoadingPageViewer()
            {
                var html = 
                    "<div style=\"text-align:center\"><div id=\"map1\""
                    + " style=\"overflow:hidden; height:100%; width:100%;"
                    + " max-width:640px; max-height:640px; text-align:center;\""
