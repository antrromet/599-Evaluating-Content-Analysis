                    {
                        dbDetailsWindow.style.display="block";
                        dbDetailsWindow.state = true;
                        tab2.style.backgroundColor
                            = dbDetailsWindow.style.backgroundColor;
                        tab2.style.border="1px solid";
                        tab2.style.borderBottom="0px";
                    }
                }
            }
