<script type="text/javascript">
if (window.globals==null){window.globals = new Object();}
if (globals.feedback==null)
{
    globals.feedback =
    {
        doHover: function()
        {
            var wrapper = document.getElementById("feedback");
            wrapper.style.marginLeft = "0";
        }
        ,doOut: function()
        {
            var wrapper = document.getElementById("feedback");
            wrapper.style.marginLeft = "-4px";
        }
        ,doClick: function()
        {
            gcmd.gwt.widgetwrappers.pageViewerApp.viewPage(
                "Feedback"
                , "/r/l/report"
                , 1160, 610);
            return false;
        }
    };
}
</script>

<div class="screenControl" id="feedback"
        onmouseover="globals.feedback.doHover()"
