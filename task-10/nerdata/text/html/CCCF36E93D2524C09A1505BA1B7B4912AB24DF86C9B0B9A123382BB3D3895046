function toggleNameValueList(tag)
{
    try
    {
        if (tag==null){return;}
        
        var disContent = document.getElementById(tag.id + "Content");
        var disMsg = document.getElementById(tag.id + "Message");
        var disImg = tag.getElementsByTagName("img")[0];
        var imgBaseSrc = disImg.src.substring(0, disImg.src.lastIndexOf("/")+1);
        
        if (this.state == null){this.state = {open:false};}
        if (this.state.open)
        {//close it
            disImg.src = imgBaseSrc + "plus.gif";
            disMsg.innerHTML = "view more";
            disContent.style.display="none";
        }
        else
        {//open it
            disImg.src = imgBaseSrc + "minus.gif";
            disMsg.innerHTML = "hide";
            disContent.style.display="block";
        }
        this.state.open = !this.state.open;
    }
    catch (e){}
    return false;
}

</script>

    <div class="fieldSection">
        <div class="field">Extended Metadata Properties</div>
        <div id="nameValueList" class="screen_control directions" onclick="toggleNameValueList(this);">
            <img src="default/images/plus.gif?portal=GCMD"/>
            <span>(Click to <span id="nameValueListMessage">view more</span>)</span>
        </div>
        <div id="nameValueListContent" style="display:none">
