            <script type="text/javascript">
                function addUrl(element)
                {
                    var currentHref = element.href;
                    if(currentHref.indexOf("{url}")!=-1)
                    {
                        var currentUri = encodeURIComponent(document.location.href);
                        element.href = currentHref.replace('{url}', currentUri);
                        console.log(element.href);
                    }
                    else
                    {
                        element.href = currentHref + encodeURIComponent(document.location.href);
                        console.log(element.href);
                    }
                }
            </script></div>
