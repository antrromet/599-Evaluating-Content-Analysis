<script type="text/javascript">
    /*
    add a link tag to the head element to assist tools looking for the
    non-dynamic version of this page.
    */
    var headList = document.getElementsByTagName("head");
    if (headList!=null && 0<headList.length)
    {
        var head = headList[0];
        var link = document.createElement("link");
        link.setAttribute("creator", "app");
        link.setAttribute("type", "text/html");
        link.setAttribute("rel", "alternate");
        link.setAttribute("href"
            , window.location.pathname
            + "?Portal=GCMD"
            + "&KeywordPath=Instruments|Earth Remote Sensing Instruments|Passive Remote Sensing|Positioning&#47;Navigation|COMPASSES"
            + "&MetadataType=1"
            + "&Output=basic"
        );
        
        
        head.appendChild(link);
    }
</script>
