    if (headList!=null && 0<headList.length)
    {
        var head = headList[0];
        var link = document.createElement("link");
        link.setAttribute("creator", "app");
        link.setAttribute("type", "text/html");
        link.setAttribute("rel", "alternate");
        link.setAttribute("href"
            , window.location.pathname
            + "?Portal=GCMD"
            + "&KeywordPath=Instruments|Earth Remote Sensing Instruments|Passive Remote Sensing"
            + "&MetadataType=1"
            + "&Output=basic"
        );
        
        
        head.appendChild(link);
    }
