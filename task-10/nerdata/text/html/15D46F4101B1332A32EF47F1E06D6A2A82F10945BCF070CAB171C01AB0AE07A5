function getSelectedValue(select) {
    var index = select.selectedIndex;
    if ((index >= 0) && (index < select.length)) {
        return select.options[index].value;
    }
    return '';
}
function submit(select) {
    if (getSelectedValue(select) != '') {
        select.form.submit();
    }
}
