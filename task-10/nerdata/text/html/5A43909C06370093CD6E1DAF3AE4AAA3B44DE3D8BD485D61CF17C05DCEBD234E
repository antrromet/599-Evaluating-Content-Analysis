<script type="text/javascript">
    function doOnLoad()
        {
            var parts = document.location.search.split("&");
            for (var p in parts)
            {
                var param = parts[p];
                if (param.indexOf("?")==0){param = param.substring(1);}
                var nameValueSep = param.indexOf("=");
               
                var nameValue = ["",""];
                if (nameValueSep<0)
                {//found no parameters
                    nameValue[0] = param;
                }
                else
                {
                    nameValue[0] = param.substring(0,nameValueSep);
                    nameValue[1] = unescape(param.substring(nameValueSep+1));
                }
               
                if (param!=null&&param.toLowerCase().indexOf("keywordpath")==0)
                {
                    var hasFreetext = nameValue[1].indexOf("Freetext=");
                    var hasRefineBy = nameValue[1].indexOf("Refine By");
                    var hasSpatial = nameValue[1].indexOf("Spatial_Coverage");
                    var	hasTemporal = nameValue[1].indexOf("Temporal_Coverage");
                    if (-1<hasFreetext && (-1<hasRefineBy || -1<hasSpatial))
                    {//show complex, hide simple
                        document.getElementById("complex").style.display = "block";
                        document.getElementById("simple").style.display = "none";
                    }
                    else if (-1<hasRefineBy && (-1<hasSpatial || -1<hasTemporal))
                    {//show complex, hide simple
                        document.getElementById("complex").style.display = "block";
                        document.getElementById("simple").style.display = "none";
                    }
                    else 
                    {//show simple, hide complex
                        document.getElementById("simple").style.display = "block";
                        document.getElementById("complex").style.display = "none";
                    }
                }
            }
        }
</script>

</head>
<body class="subpage" onLoad="doOnLoad()";>





