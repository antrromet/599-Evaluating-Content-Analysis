            function mobileCookie() {
                        
                var theDate = new Date();
                var oneYearLater = new Date(theDate.getTime() + 31536000000);
                var expiryDate = oneYearLater.toGMTString();
            
                document.cookie = 'eoMobile=false; expires='+expiryDate+'; path=/';
                setTimeout("window.location = 'http://earthobservatory.nasa.gov/NaturalHazards/view.php?id=51064&eocn=image&eoci=morenh'",500);
            
            }
            
