<script type="text/javascript">

/*function showPropInfo(text)
{
    var html = "<div>" + text + "</div>";
    var id = "info" + Math.random();
    gcmd.gwt.widgetwrappers.pageViewerApp.setWindow(id
        , "Property Information"
